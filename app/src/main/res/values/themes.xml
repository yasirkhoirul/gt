<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Base.Theme.MyStoryApp" parent="Theme.Material3.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/ijomuda</item>
        <item name="colorAccent">@color/ijomuda</item>
        <item name="colorPrimaryVariant">@color/ijomuda</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/ijomuda</item>

    </style>
    <style name="roundedImageView">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">100dp</item>
    </style>

    <style name="CustomOutlinedBox" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeWidthFocused">1dp</item>
        <item name="boxStrokeWidth">1dp</item>
        <item name="boxStrokeColor">@color/primer</item>
        <item name="boxCornerRadiusBottomEnd">24dp</item>
        <item name="boxCornerRadiusBottomStart">24dp</item>
        <item name="boxCornerRadiusTopEnd">24dp</item>
        <item name="boxCornerRadiusTopStart">24dp</item>
    </style>

    <style name="CustomOutlinedBox2" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxStrokeColor">#E0E0E0</item>
        <item name="boxCornerRadiusBottomEnd">8dp</item>
        <item name="boxCornerRadiusBottomStart">8dp</item>
        <item name="boxCornerRadiusTopEnd">8dp</item>
        <item name="boxCornerRadiusTopStart">8dp</item>
    </style>

    <style name="CustomOutlinedBox3" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">
        <item name="boxCornerRadiusBottomEnd">16dp</item>
        <item name="boxCornerRadiusBottomStart">16dp</item>
        <item name="boxCornerRadiusTopEnd">16dp</item>
        <item name="boxCornerRadiusTopStart">16dp</item>
    </style>

    <style name="CustomOutlinedBoxDropDown" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="boxStrokeColor">@color/black40</item>
        <item name="boxCornerRadiusBottomEnd">24dp</item>
        <item name="boxCornerRadiusBottomStart">24dp</item>
        <item name="boxCornerRadiusTopEnd">24dp</item>
        <item name="boxCornerRadiusTopStart">24dp</item>
    </style>

    <style name="CustomOutlinedBoxDropDown2" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox.ExposedDropdownMenu">
        <item name="boxStrokeColor">@color/black40</item>
        <item name="strokeWidth">0dp</item>
    </style>


